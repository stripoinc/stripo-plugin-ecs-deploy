---
# üîê Single source of truth for all passwords in Stripo ECS project
# IMPORTANT: This file should be encrypted using ansible-vault
# Command to encrypt: ansible-vault encrypt group_vars/passwords.yml
# Command to view: ansible-vault view group_vars/passwords.yml
# Command to edit: ansible-vault edit group_vars/passwords.yml
#
# Copy this file to passwords.yml and fill in your actual values!

# PostgreSQL passwords for all databases
postgresql_passwords:
  # Main Stripo Plugin services
  stripo_plugin_local_bank_images: "your-password-bank-images"
  stripo_plugin_local_custom_blocks: "your-password-custom-blocks"
  stripo_plugin_local_documents: "your-password-documents"
  stripo_plugin_local_drafts: "your-password-drafts"
  stripo_plugin_local_html_gen: "your-password-html-gen"
  stripo_plugin_local_plugin_details: "your-password-plugin-details"
  stripo_plugin_local_plugin_stats: "your-password-plugin-stats"
  stripo_plugin_local_securitydb: "your-password-securitydb"
  stripo_plugin_local_timers: "your-password-timers"
  countdowntimer: "your-password-countdowntimer"
  ai_service: "your-password-ai-service"

# PostgreSQL root password
postgresql_root_password: "your-super-secret-postgres-password"

# Redis password
redis_password: "your-redis-password"

# Countdown Timer settings
countdown_timer:
  username: "Admin"
  password: "your-timer-password"
