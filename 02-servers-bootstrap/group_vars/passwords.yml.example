---
# üîê Single source of truth for all passwords in Stripo ECS project
# IMPORTANT: This file should be encrypted using ansible-vault
# Command to encrypt: ansible-vault encrypt group_vars/passwords.yml
# Command to view: ansible-vault view group_vars/passwords.yml
# Command to edit: ansible-vault edit group_vars/passwords.yml

# PostgreSQL passwords for all databases
postgresql_passwords:
  # Main Stripo Plugin services
  stripo_plugin_local_bank_images: "password_bank_images"
  stripo_plugin_local_custom_blocks: "password_custom_blocks"
  stripo_plugin_local_documents: "password_documents"
  stripo_plugin_local_drafts: "password_drafts"
  stripo_plugin_local_html_gen: "password_html_gen"
  stripo_plugin_local_plugin_details: "password_plugin_details"
  stripo_plugin_local_plugin_stats: "password_plugin_stats"
  stripo_plugin_local_securitydb: "password_securitydb"
  stripo_plugin_local_timers: "password_timers"
  countdowntimer: "password_countdowntimer"
  ai_service: "password_ai_service"

# PostgreSQL root password
postgresql_root_password: "password"

# Redis password
redis_password: "password"

# Countdown Timer settings
countdown_timer:
  username: "Admin"
  password: "password"

# Application secrets
application_secrets:
  jwt_secret: "token_secret"
  countdown_secret_key: "secret_key"
